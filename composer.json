{
  "name": "Genniuz Framework",
  "description": "Estrutura com algumas configurações e automatizações de processos e tarefas para iniciar projetos em PHP de forma simples e rapida.",
  "keywords": ["project", "framework", "structure", "mvc"],
  "homepage": "https://github.com/lleocastro/genniuz-framework",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Léo Castro",
      "email": "leonardo_carvalho@outlook.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "aura/router": "^3.0",
    "doctrine/orm": "2.5.*",
    "zendframework/zend-escaper": "2.5.2",
    "zendframework/zend-diactoros": "1.3.7",
    "symfony/event-dispatcher": "v3.2.0-RC2",
    "symfony/translation": "2.8.*|3.0.*",
    "symfony/var-dumper": "2.8.*|3.0.*",
    "classpreloader/classpreloader": "~3.0",
    "phpunit/php-invoker": "1.1.4",
    "monolog/monolog": "~1.11",
    "slim/slim": "3.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "autoload": {
    "psr-4": {
      "Bootstrap\\": "bootstrap/",
      "Resources\\Helpers\\": "resources/helpers/",
      "System\\": "system/",
      "System\\Interfaces\\": "system/interfaces/"
    }
  }
}