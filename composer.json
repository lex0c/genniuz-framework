{
  "name": "lleocastro/genniuz-framework",
  "description": "Ambiente com algumas configurações e automatizações de processos e tarefas para iniciar projetos em PHP de forma simples e rapida.",
  "keywords": ["project", "framework", "structure", "mvc", "automation", "backend", "frontend"],
  "homepage": "https://github.com/lleocastro/genniuz-framework",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Léo Castro",
      "email": "leonardo_carvalho@outlook.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "aura/router": "3.0.*",
    "doctrine/orm": "2.5.*",
    "zendframework/zend-escaper": "2.5.*",
    "zendframework/zend-diactoros": "1.3.*",
    "symfony/event-dispatcher": "v3.2.0-RC2",
    "symfony/translation": "3.0.*",
    "symfony/var-dumper": "3.0.*",
    "classpreloader/classpreloader": "3.0.*",
    "phpunit/php-invoker": "1.1.*",
    "monolog/monolog": "1.22.*",
    "nesbot/carbon": "~1.21.0",
    "slim/slim": "3.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "symfony/css-selector": "3.0.*",
    "symfony/dom-crawler": "3.0.*"
  },
  "autoload": {
    "psr-4": {
      "System\\": "system/",
      "Loaders\\": "loaders/",
      "Resources\\Helpers\\": "resources/helpers/",
      "System\\Interfaces\\": "system/interfaces/",
      "System\\Core\\Logger\\": "system/core/logger/"
    }
  },
  "support": {
    "issues": "https://github.com/lleocastro/genniuz-framework/issues",
    "source": "https://github.com/lleocastro/genniuz-framework"
  },
  "config": {
    "vendor-dir": "vendors",
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {},
  "minimum-stability": "beta",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "system/plugins": []
    }
  }
}